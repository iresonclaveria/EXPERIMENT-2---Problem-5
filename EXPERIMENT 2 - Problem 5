#include <conio.h>
#include <iostream>
using namespace std;
int main()
{
    int Multiplier = 1, Number = 1;
    
    while (Number <=30)
    {
        cout<<Number;
        Number = Number + Multiplier;
        
        if (Number >=10)
        {
            Multiplier = 2;
        }
        
        if (Number <=30)
        {
            cout<<",";
        }
    }
_getch();
return 0;
}
